  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}

layer {
  name: "slice"
  type: "Slice"
  bottom: "conv1_2"
  top: "conv1_2_data"
  top: "conv1_2_target"
  slice_param {
    axis: 0
    slice_point: 1
  }
}
