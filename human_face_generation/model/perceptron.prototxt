  dim: 1
  dim: 512
  dim: 17
  dim: 17
}

layer {
 name: 'loss1'
 type: "Python"
 bottom: "data"
 bottom: "target_data"
 bottom: "mask1"
 top: 'Perceptualloss1'
 loss_weight: 1
 python_param {
   module: "python_loss_layers"
   layer: "L1LossMaskLayer"
   param_str: "{\'loss_weight\': 1.0}"
 }
}

layer {
  name: "concat"
  type: "Concat"
  bottom: "data"
  bottom: "target_data"
  top: "data_input"
  concat_param {
    axis: 0
  }
}

###################### DeepLab ####################
layer {
  name: "conv1_1"
  type: "Convolution"
  bottom: "data_input"
  top: "conv1_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "relu1_1"
  type: "ReLU"
  bottom: "conv1_1"
  top: "conv1_1"
}
layer {
  name: "conv1_2"
  type: "Convolution"
  bottom: "conv1_1"
  top: "conv1_2"
  param {
    lr_mult: 1
