force_backward: true

input: "data"
input_shape {
  dim: 1
  dim: 3
  dim: 129
  dim: 129
}

input: "target_data"
input_shape {
  dim: 1
  dim: 3
  dim: 129
  dim: 129
}

input: "mask1"
input_shape {
  dim: 1
  dim: 3
  dim: 129
  dim: 129
}

input: "mask2"
input_shape {
  dim: 1
  dim: 64
  dim: 129
  dim: 129
}

input: "mask3"
