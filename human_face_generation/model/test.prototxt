force_backward: true
input: "data1"
input_shape {
  dim: 1
  dim: 68
  dim: 128
  dim: 128
}

input: "data2"
input_shape {
  dim: 1
  dim: 68
  dim: 64
  dim: 64
}

input: "data3"
input_shape {
  dim: 1
  dim: 68
  dim: 32
  dim: 32
}

input: "data4"
input_shape {
  dim: 1
  dim: 68
  dim: 16
  dim: 16
}

input: "data5"
input_shape {
  dim: 1
  dim: 68
  dim: 8
  dim: 8
}

input: "data6"
input_shape {
  dim: 1
  dim: 68
  dim: 4
  dim: 4
}

input: "sto_seed1"
input_shape {
  dim: 1
  dim: 128
  dim: 1
  dim: 1
}

input: "sto_seed2"
input_shape {
  dim: 1
  dim: 128
  dim: 1
  dim: 1
}

# stage 6
layer {
  name: "conv6_1"
  type: "Convolution"
  bottom: "data6"
  top: "conv6_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 1024
    kernel_size: 3
    stride: 1
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.002
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu6_1"
  type: "ReLU"
  bottom: "conv6_1"
  top: "conv6_1"
  relu_param {
    negative_slope: 0.2
  }
}
layer {
  name: "conv6_2"
  type: "Convolution"
  bottom: "conv6_1"
  top: "conv6_2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 1024
    kernel_size: 3
    stride: 1
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.002
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu6_2"
  type: "ReLU"
  bottom: "conv6_2"
  top: "conv6_2"
  relu_param {
    negative_slope: 0.2
  }
}

layer {
  name: "upsample_deconv6"           
  type: "Deconvolution"
  bottom: "conv6_2"
  top: "deconv6_2"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 1024
    pad: 1
    kernel_size: 4
    stride: 2
#    engine: CUDNN
  }
}

layer {
  name: "concat6"
  type: "Concat"
  bottom: "data5"
  bottom: "deconv6_2"
  top: "concat6"
  concat_param {
    axis: 1
  }
}

# stage 5
